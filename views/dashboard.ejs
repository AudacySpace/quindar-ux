<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, shrink-to-fit=no, initial-scale=1">
  <meta name="description" content="dashboard for mission operations platform, Quindar">
  <meta name="author" content="Chavi Malhotra">
  <meta name="author" content="Taruni Gattu">
  <title>Quindar Dashboard</title>
  <link href="/css/bootstrap-min.css" rel="stylesheet">
  <link href="/css/font-awesome.min.css" rel="stylesheet">
  <link href="/css/dashboardstyles.css" rel="stylesheet">
  <link rel="stylesheet" href="/css/gridstack.css"/>
  <link href="/css/widgetstyles.css" rel="stylesheet">
  <link href="/css/positions.css" rel="stylesheet"> 
  <link href="/css/datatablestyles.css" rel="stylesheet"> 
  <link href="/css/bootstrap-datetimepicker.min.css">
 </head>

<body onload="startTime()">
  <div id="wrapper" class="toggled">
    <!--Left side nav bar -->
    <div id="mySidenav" class="sidenav">
      <a href="javascript:void(0)" class="closebtn pull-right" id="closebtn" onclick="closeLeftNav()">&times;</a>
      <form class="navbar-form" id="form1">
        <div class="form-group"> <!-- style="display:inline;" -->
          <div class="input-group" id="searchbar">
            <input type="text" class="form-control" id="input-search" placeholder="Search">
            <span class="input-group-addon" id="group-search">
              <span class="glyphicon glyphicon-search"></span>
            </span>
          </div>
        </div>
      </form>
      <a class="leftmenu" href="#" id="left-menu-item1">Realtime Telemetry</a>
<!--       <ul>
        <li><a href="#">Velocity</a></li>
          <ul id="velocity-list">
            <li class="velocity-list-item">v</li>
            <li class="velocity-list-item">vx</li>
            <li class="velocity-list-item">vy</li>
            <li class="velocity-list-item">vz</li>
          </ul>
      </ul> -->
      <a class="leftmenu" href="#" id="left-menu-item2">Sat Subsystems</a>
      <a class="leftmenu" href="#" id="left-menu-item3">Sat Imagery</a>
      <a class="leftmenu" href="#" id="left-menu-item4">Atmosphere Pressure</a>
    </div>
    <!-- End of Left Side Nav -->

    <!-- Main Dashboard Navigation-->
    <div id="main">
      <nav class="navbar navbar-default" id="navbar">
        <div class="container-fluid" id="top-header">
          <div class="navbar-header">
           <a class="navbar-brand" href="#" id="QRegular" onclick="openLeftNav()"><img id="main-img" src="/media/logos/quindar_regular.svg" alt="Quindar"/></a>
           <a class="navbar-brand" href="#" id="QBlack" onclick="openLeftNav()"><img id="main-img" src="/media/logos/Quindar_Icon_Black.svg" alt="Quindar"/></a>
         </div>
         <ul class="nav navbar-nav">
          <li class="statusicons"><ul class="nav navbar-nav status-bar" id="s-icons">
            <li id="icon-05"><a href="#" class="status-icon"><img id="i5" src="/media/icons/status_icons-05.png" alt="Quindar"/></a></li>
            <li id="icon-06"><a href="#" class="status-icon"><img id="i6" src="/media/icons/status_icons-06.png" alt="Quindar"/></a></li>
            <li id="icon-07"><a href="#" class="status-icon"><img id="i7" src="/media/icons/status_icons-07.png" alt="Quindar"/></a></li>
            <li id="icon-08"><a href="#" class="status-icon"><img id="i8" src="/media/icons/status_icons-08.png" alt="Quindar"/></a></li>
          </ul></li>
          <li><ul class="nav navbar-nav time">
            <li class="clock" id="txt"></li>
          </ul></li>
        </ul>
        <ul class="nav navbar-nav navbar-right">
          <li>
            <a class="dropdown-toggle"  id="username" data-toggle="dropdown" href="#"><%= user.google.name %></a>
            <ul class="dropdown-menu logout-bar">
              <li ><a href="/logout">Logout</a></li>
            </ul>
          </li>
          <li id="cfgmenu"><a href="#" id="add-widget" onclick="openRightNav()"> 
            <span class="fa fa-bars" aria-hidden="true"> </span>
          </a></li>
        </ul>
      </div>
    </nav>
  </div>
  <!--End Main Dashboard Navigation-->

  <!--Main Dashboard for Widgets -->
  <div class="container">
   <!--  <h5 id="dashboard">Dashboard</h5> -->
    <h6 id="usermail"><%=user.google.email %></h6>
    <div class="grid-stack" id="container">
    </div>
  </div>

  <!-- Dashboard Footer -->
  <footer class="footer">
    <div class="container">
      <p class="text-muted"></p>
    </div>
  </footer>
  <!--End Dashboard Footer -->

  <!--Right Side Nav -->
  <div id="myrightSidenav" class="rightsidenav">
    <h3 id="user-name">Hi <%=user.google.name %></h3>
    <a href="javascript:void(0)" class="rclosebtn pull-right rmenu" id="rclosebtn" onclick="closeRightNav()">&times;</a>
    <a href="#" id="configmenu" class="rmenu"> Configuration Menu</a>
    <a href="#" onclick = "openQwidgetMenu()" id="quindar-widgets" class="rmenu"><span class="glyphicon glyphicon-menu-down pull-right" id="qwidget-arrow"></span><span class="glyphicon glyphicon-modal-window" id="qwidgets"></span>Qwidgets
    </a>
      <ul id="qwidgetMenu" style="display:none;">
        <li><a id="addQwidget" href="#" onclick = "openQwidgetList()"><span class="glyphicon glyphicon-plus" id="qwidgetPlus"></span>Add Qwidget<span class="glyphicon glyphicon-menu-down pull-right" id="qwidget-add"></a> 
        </li>
          <ul id="widgettops" style="display:none;">   
            <li><a id="addGround" href="#"><span class="glyphicon glyphicon-globe" id="g-track"></span>Ground Track</a>
            </li>
			     <li><a id="addLine" href="#"><i class="fa fa-line-chart" aria-hidden="true" id="l-plot"></i>Line Plot</a>
            </li>
            <li><a id="addtablewidget" href="#"><span class="fa fa-table" aria-hidden="true" id="t-table"></span>Telemetry Table</a>
            </li>
            <li><a id="searchId" href="#"><span class="glyphicon glyphicon-search" id="s-table"></span>Search By Id</a>
            </li>
            <li><a id="datatable" href="#" onclick="addDataTable();"><span class="fa fa-table" id="d-table"></span>Data Table</a>
            </li>
          </ul>
        <li><a id="saveLayout" href="#"><span class="glyphicon glyphicon-save-file" id="save-layout"></span>Save Layout</a></li>
        <li><a id="loadLayout" href="#"><span class="glyphicon glyphicon-open-file" id="load-layout"></span>Load Layout<span class="glyphicon glyphicon-menu-down pull-right" id="layout-arrow"></span></a></li>
        <li><a id="clearLayout" href="#"><span class="glyphicon glyphicon-trash" id="clear-layout"></span>Clear</a></li>
      </ul>
  
    <a href="#" id="user" class="rmenu"><span class="glyphicon glyphicon-menu-down pull-right" id="user-admin-arrow"></span><span class="glyphicon glyphicon-user" id="user-admin"></span>User Administration</a>
    <a href="#" id="server" class="rmenu"><span class="glyphicon glyphicon-menu-down pull-right" id="server-admin-arrow"></span><i class="fa fa-server" aria-hidden="true" id="server-admin"></i></span>Server Administration</a>
    <a href="#" id="document" class="rmenu"><span class="glyphicon glyphicon-menu-down pull-right" id="doc-arrow"></span><i class="fa fa-file-text-o" aria-hidden="true" id="docs"></i>Documentation</a>
    <a href="/logout" id="mini-logout" class="rmenu">Logout</a>
  </div>
  <!--End of Right Side Nav -->

  <!-- jQuery -->
  <script src="/scripts/jquery.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.11.0/jquery-ui.js"></script>
  <!-- Bootstrap Core JavaScript -->
  <script src="/scripts/bootstrap.min.js"></script>
  <!-- lodash -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/lodash.js/3.5.0/lodash.min.js"></script>
  <!-- gridstack library -->
  <script src="/scripts/dist/gridstack.all.js"></script>
  <!-- UTC Clock -->
  <script src="/scripts/clock.js"></script>
  <!-- Calendar -->
  <script src="/scripts/moment-with-locales.min.js"></script>  
  <script src="/scripts/bootstrap-datetimepicker.min.js"></script>
  <!-- Side Navigation -->
  <script src="/scripts/side-navigation.js"></script>
  <!-- D3 -->
  <script src="/scripts/d3.min.js"></script>
  <!-- topojson-client -->
  <script src="/scripts/topojson-client.min.js"></script>
  <!-- Dashboard widget grid layout-->
  <!-- <script src="/scripts/widgetgrid.js"></script> -->
  <script src="/scripts/dashboard.js"></script>
  <script src="/scripts/datatableqwidget.js"></script>
  <script src="/scripts/categoryid-menu.js"></script>
  </body>
  </html>
